mixin default-pricing
  .range.range-xs-center.range-lg.range-30

    // Pricing Box
    .cell-sm-6.cell-lg-3
      .pricing-box.pricing-box-center.pricing-box-novi
        .pricing-box-header: h4 basic
        .pricing-box-body
          ul.pricing-box-list
