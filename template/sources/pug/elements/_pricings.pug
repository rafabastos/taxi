mixin xxl-pricing
  .range.range-md.range-50.range-xs-center

    // Pricing Box XL
    .cell-sm-6.cell-lg-3
      .box-price-info
        .box-price-info-image
          +png('car-01-210x72')

      .pricing-box.pricing-box-xl.pricing-box-novi
        .pricing-box-header
          h4 basic
        .pricing-box-price
          .heading-2
            sup $
            | 1.25/km
        a.button.button-sm.button-secondary.button-nina(href='#') order now
        .pricing-box-body
          ul.pricing-box-list
            li
              .unit.unit-spacing-sm.unit-horizontal.unit-middle
                .unit-left: span.icon.icon-md-big.icon-primary.mdi.mdi-earth
                .unit-body: span Lots of locations
            li
              .unit.unit-spacing-sm.unit-horizontal.unit-middle
